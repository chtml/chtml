{
  "name": "moto",
  "version": "0.0.0",
  "description": "",
  "main": "lib/index.js",
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-loader": "6.x.x",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-es2016": "6.x.x",
    "babel-preset-es2017": "6.x.x"
  },
  "scripts": {
    "dev": "npm run watch-build",
    "copy": "rm -rf lib && cp -r src lib",
    "build": "npm run lint && npm run copy && babel --presets es2015,es2016,es2017 --plugins transform-runtime src/ --out-dir lib/ --source-maps inline",
    "watch-build": "npm run build -- --watch",
    "watch": "npm run watch-build",
    "lint": "standard 'src/**/**.js' --fix",
    "prepublish": "npm run lint && npm run build"
  },
  "author": "",
  "license": "MIT"
}
